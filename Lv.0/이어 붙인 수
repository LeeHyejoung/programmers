class Solution {
    public int solution(int[] num_list) {
        int answer = 0;
        String oddStr = "", evenStr = "";
        
        for (int i = 0; i < num_list.length; i++) {
            if (num_list[i] % 2 == 1)
                oddStr += num_list[i];
            else
                evenStr += num_list[i];
        }
        
        answer = Integer.parseInt(oddStr) + Integer.parseInt(evenStr);
        
        return answer;
    }
}
