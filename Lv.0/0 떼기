class Solution {
    public String solution(String n_str) {
        String answer = "";
        int flag = 0;
        
        int i;
        for (i = 0; i < n_str.length(); i++) {
            if (n_str.charAt(i) != '0') {
                flag = 1;
            }            
            if (flag == 1) {
                answer += n_str.charAt(i);
            }
        }
        
        return answer;
    }
}
