import java.util.*;

class Solution {
    public int solution(int[] array) {
        int answer = 0;
        ArrayList<Integer> list = new ArrayList<Integer>();
        
        for (int i = 0; i < array.length; i++)
            list.add(array[i]);
        
        Collections.sort(list);
        answer = list.get(array.length / 2);
        
        return answer;
    }
}
